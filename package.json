    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.71.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vitest": "^1.4.0",
    "vue-tsc": "^1.8.27"
